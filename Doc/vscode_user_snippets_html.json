{	
	"div with class": {
		"prefix": "div",
		"body": [
			"<div class=\"$1\">$2</div>"
		],
		"description": "div class"
	},
	"html5-layui": {
		"prefix": "html5",
		"body": [
			"<!DOCTYPE html>",
			"<html>",
			"<head>",
			"    <meta charset=\"UTF-8\">",
			"    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0, maximum-scale=1\">",
			"    <title>$1</title>",
			"    <meta name=\"renderer\" content=\"webkit\">",
			"    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge,chrome=1\">",
			"    <meta name=\"apple-mobile-web-app-status-bar-style\" content=\"black\">",
			"    <meta name=\"apple-mobile-web-app-capable\" content=\"yes\">",
			"    <meta name=\"format-detection\" content=\"telephone=no\">",
			"    <link rel=\"stylesheet\" href=\"$2\" media=\"all\">",
			"</head>",
			"<body>",
			"$3",
			"</body>",
			"</html>"
		],
		"description": "html5-layui"
	},
	"fieldset with class": {
		"prefix": "fieldset",
		"body": [
			"<fieldset class=\"layui-elem-field$1\">",
			"    <legend>$2</legend>",
			"</fieldset>"
		],
		"description": "fieldset legend"
	},
	"layui-button with class": {
		"prefix": "button",
		"body": [
			"<button type=\"button\" class=\"layui-btn$1\">$2</button>"
		],
		"description": "layui-button with class"
	}
	,
	"layui-icon with class": {
		"prefix": "icon",
		"body": [
			"<i class=\"layui-icon$1\"></i>"
		],
		"description": "layui-button with class"
	},
	
		"layui-label with class":{
			"prefix":"labell",
			"body":[
				"<label class=\"layui-form-label\">$1</label>"
			],
			"description":"layui label with class"
		}
		,
	
		"layui div form item":{
			"prefix":"divitem",
			"body":[
				"<div class=\"layui-form-item\">",
				"    $1",
				"</div>"
			],
			"description":"layui div form item"
		}
		,
	
		"layui div input block":{
			"prefix":"divblock",
			"body":[
				"<div class=\"layui-input-block\">",
				"    $1",
				"</div>"
			],
			"description":"layui div input block"
		}
		,
	
		"layui div input inline":{
			"prefix":"divinline",
			"body":[
				"<div class=\"layui-input-inline\">",
				"    $1",
				"</div>"
			],
			"description":"layui div input inline"
		}

}